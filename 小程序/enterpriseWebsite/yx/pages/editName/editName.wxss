@import "../../utils/yx.wxss";

page{ background-color:#fff;}
.label{ font-size:32rpx; color:#666; height:90rpx; justify-content:flex-start; margin-left:30rpx;}
.label text{ font-size:32rpx; color:#ff0000; margin-right:2rpx;}
.input{ font-size:32rpx; width:690rpx; height:100rpx; background-color:#f5f5f5; margin:0 auto; padding-left:20rpx;}
.add{ width:200rpx; height:200rpx; position:relative; margin-left:30rpx;}
.add image{ width:200rpx; height:200rpx;}
.add view{ width:60rpx; height:60rpx; background-image:url(http://q.img.soukong.cn/website/yx/close_gray.png); background-size:30rpx 30rpx; background-repeat:no-repeat; background-position:center center; position:absolute; right:0; top:0;}
.btn{ font-size:34rpx; font-weight:bold; color:#fff; width:100%; height:98rpx; position:fixed; left:0; bottom:0; background-color:#5cb8f7;}

.place_style{ font-size:32rpx; color:#bac6d2;}