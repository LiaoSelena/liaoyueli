@import "../../utils/yx.wxss";

.total{ font-size:26rpx; color:#5cb8f7; height:68rpx; justify-content:flex-start; position:relative; padding-left:46rpx;}
.total text{ width:4rpx; height:26rpx; background-color:#5cb8f7; border-radius:6rpx; position:absolute; left:30rpx; top:50%; margin-top:-13rpx;}
.list{ width:710rpx; height:164rpx; margin:0 20rpx 10rpx; background-color:#fff; border-radius:10rpx; box-shadow:0 0 10rpx #eee;}
.list_left{ height:164rpx; float:left;}
.list_left image{ width:98rpx; height:98rpx; border-radius:50%; margin:0 30rpx;}
.list_left_box{ height:98rpx; justify-content:space-between;}
.list_text1{ font-size:30rpx; width:250rpx;}
.list_text2{ color:#666;}
.list_text3{ font-size:22rpx; color:#666;}
.list_right{ height:164rpx; float:right; margin-right:20rpx;}
.list_right view,.list_right navigator{ font-size:24rpx; width:116rpx; height:54rpx; border-radius:10rpx;}
.list_card{ color:#5cb8f7; border:2rpx solid #5cb8f7; background-color:#fff;}
.list_remove{ color:#fff; border:2rpx solid #5cb8f7; background-color:#5cb8f7; margin-left:10rpx;}
.more{ color:#666; height:68rpx;}
.nothing{ overflow:hidden;}
.nothing image{ width:320rpx; height:180rpx; margin:140rpx auto 30rpx;}
.nothing view{ font-size:32rpx; color:#999; text-align:center;}