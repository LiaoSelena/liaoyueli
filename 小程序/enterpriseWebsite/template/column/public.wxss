.c_box{ background-color:#fff; padding:54rpx 30rpx; margin-top:20rpx;}
.c_hd{ height:60rpx; position:relative; margin-bottom:40rpx;}
.c_hd_text{ font-size:34rpx; font-weight:bold; float:left;}
.c_hd_control{ width:234rpx; height:60rpx; float:right;}
.c_hd_control_left,.c_hd_control_right{ color:#28b9fd; width:90rpx; height:60rpx; align-items:flex-start; justify-content:space-between;}
.c_hd_control_left{ float:left;}
.c_hd_control_left image{ width:28rpx; height:29rpx;}
.c_hd_control_right{ float:right;}
.c_hd_control_right image{ width:27rpx; height:27rpx;}
.c_more{width: 150rpx;}
.c_more image{width: 11rpx;height: 20rpx;margin-right: 10rpx;margin-top: 5rpx;}
.c_line{ width:56rpx; height:6rpx; background-color:#28b9fd; border-radius:10rpx; box-shadow:3rpx 3rpx 10rpx rgba(40,185,253,0.5); position:absolute; left:0; bottom:3rpx;}

/* 简介模板 */
.i_box{ overflow:hidden; position:relative;}
.i_mask{ width:750rpx; height:160rpx; background:linear-gradient(rgba(255,255,255,0),rgba(255,255,255,0.96)); position:absolute; left:0; bottom:0;}
.i_template{ margin-bottom:30rpx;}
.i_template:last-child{ margin-bottom:0;}
.i_text{ font-size:32rpx; color:#666; line-height:1.8; text-align:justify; overflow:hidden;}
.i_see_more{ font-size:28rpx; color:#28b9fd; height:96rpx; padding:30rpx 0; background-color:#fff;}
.i_see_more image{ width:25rpx; height:22rpx; margin-right:10rpx; transition:0.6s;}
.i_img{ width:690rpx; height:388rpx; border-radius:10rpx;}
.i_template .i_img{ margin-bottom:30rpx;}
.i_template .i_img:last-child{ margin-bottom:0;}
.i_cover{ width:690rpx; height:388rpx; position:relative;}
.i_cover_video{ width:690rpx; height:388rpx;}
.i_cover_play{ width:130rpx; height:130rpx; position:absolute; left:50%; margin-left:-65rpx; top:50%; margin-top:-65rpx;}

.i_see_more_icon{ transform:rotate(180deg);}

/* 资讯模板 */
.n_list{ border-bottom:2rpx solid #e8e8e8; justify-content:space-between; overflow:hidden; padding-top:30rpx; padding-bottom:30rpx;}
.n_list:first-child{ padding-top:0;}
.n_list:last-child{ border-bottom:0; padding-bottom:0;}
.n_list image{ width:215rpx; height:154rpx; border-radius:10rpx; float:left;}
.n_list_right{ width:440rpx; height:154rpx; float:right; padding:6rpx 0; justify-content:space-between;}
.n_list_text{ font-size:34rpx;}
.n_list_time{ font-size:24rpx; color:#999;}

/* 产品模板 */
.p_box{ overflow:hidden;}
.p_list{ width:332rpx; height:478rpx; float:left; margin-right:24rpx;}
.p_list:nth-of-type(even){ margin-right:0;}
.p_list image{ width:332rpx; height:360rpx; border-radius:10rpx 10rpx 0 0;}
.p_list_box{ height:118rpx;}
.p_list_money{ font-size:34rpx; font-weight:bold; color:#fa4b3e;}
.p_list_money text{ color:#fa4b3e;}

/* 领军人物模板 */
.h_img{ width:100%; height:100%; border-radius:10rpx; margin-bottom:20rpx;}
.h_text{ font-size:32rpx; color:#666; line-height:1.8; text-align:justify;}
.h_cover{ width:690rpx; height:388rpx; position:relative; margin-top:20rpx;}
.h_cover_video{ width:690rpx; height:388rpx;}
.h_cover_play{ width:130rpx; height:130rpx; position:absolute; left:50%; margin-left:-65rpx; top:50%; margin-top:-65rpx;}


/* 管理团队模板 */
.t_scroll{ width:690rpx; height:443rpx; margin-right:30rpx; white-space:nowrap;}
.t_list{ display:inline-block; position:relative; margin-right:10rpx;}
.t_list:last-child{ margin-right:0;}
.t_list image{ width:333rpx; height:443rpx; border-radius:10rpx;}
.t_name{ font-size:34rpx; font-weight:bold; color:#fff; max-width:300rpx;}
.t_job{ font-size:24rpx; font-weight:bold; color:#fff; max-width:300rpx;}
.t_cz{white-space: normal;height: 60rpx;}
.t_mask{ width:333rpx; height:133rpx; background:linear-gradient(rgba(0,0,0,0),rgba(0,0,0,0.6)); padding-left:20rpx; position:absolute; left:0; bottom:0;}

/* 合作伙伴模板 */
.pa_box{ overflow:hidden;}
.pa_list{ width:230rpx; height:196rpx; float:left; align-items:center;}
.pa_list image{ width:130rpx; height:130rpx; border-radius:50%; margin-bottom:16rpx;}
.pa_list view{ font-size:24rpx; color:#666; width:230rpx; text-align:center;}

/* 加入我们模板 */
.j_text{ color:#666; line-height:1.4;}
.j_list{ height:178rpx; border-bottom:2rpx solid #e6edf2; justify-content:space-between;}
.j_list:last-child{ border-bottom:0;}
.j_list_left{ width:400rpx;}
.j_list_job{ font-size:30rpx; font-weight:bold; margin-bottom:10rpx;}
.j_list_tip{ font-size:24rpx; color:#666;}
.j_list_money{ font-size:32rpx; font-weight:bold; color:#fa4b3e;}
.j_hr{ width:690rpx; height:180rpx; border-radius:10rpx; background-image:url(http://q.img.soukong.cn/website/yx/mag_hr.jpg); background-size:100%; justify-content:space-between;}
.j_hr_left{ height:130rpx; margin-left:33rpx;}
.j_hr_img{ width:130rpx; height:130rpx; background-color:#fff; border-radius:50%; float:left; margin-right:18rpx;}
.j_hr_img image{ width:118rpx; height:118rpx; border-radius:50%;}
.j_hr_text{ width:340rpx; height:130rpx; float:left;}
.j_hr_name{ justify-content:flex-start; margin-bottom:10rpx;}
.j_hr_name view{ font-size:32rpx; font-weight:bold; color:#fff; max-width:280rpx; margin-right:10rpx;}
.j_hr_name text{ font-size:22rpx; color:#fff; width:60rpx; height:34rpx; border:2rpx solid #fff; border-radius:6rpx;}
.j_hr_phone{ justify-content:flex-start;}
.j_hr_phone view{ font-size:32rpx; color:#fff; max-width:280rpx; margin-right:14rpx;}
.j_hr_phone image{ width:30rpx; height:30rpx;}
.j_hr_code{ height:130rpx; margin-right:38rpx;}
.j_hr_code image{ width:94rpx; height:94rpx; margin-bottom:4rpx;}
.j_hr_code view{ font-size:20rpx; color:#fff; text-align:center;}

/* 联系我们模板 */
.u_map{ width:690rpx; height:388rpx;}
.u_box{ height:240rpx; margin-top:50rpx;}
.u_left{ width:480rpx; height:240rpx; float:left; justify-content:space-between; line-height:1.4;}
.u_left view{ font-size:26rpx; color:#666;}
.u_left view text{ font-size:26rpx; color:#333;}
.u_right{ height:240rpx; float:right; justify-content:space-between;}
.u_right image{ width:178rpx; height:178rpx; border-radius:50%;}
.u_right view view{ font-size:24rpx; color:#666; text-align:center;}